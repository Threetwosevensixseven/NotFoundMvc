<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>NotFoundMvc</id>
    <version>1.3.1</version>
    <title>NotFound MVC</title>
    <authors>Andrew Davey, Bojan Rajkovic</authors>
    <owners>Bojan Rajkovic</owners>
    <description>
      Provides a user-friendly '404' page whenever a controller, action or route is not found in your ASP.NET MVC application.
      A view called NotFound is rendered instead of the default ASP.NET error page.
    </description>
    <projectUrl>https://github.com/bojanrajkovic/notfoundmvc</projectUrl>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <tags>asp.net mvc web .net40</tags>
    <language>en-US</language>
  </metadata>
